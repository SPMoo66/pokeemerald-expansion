script Mart_EventScript_EvoClerk {
	lock
	faceplayer
	specialvar (VAR_TEMP_1, GetFrontierBattlePoints)
	if (var(VAR_TEMP_1) == 0) {
		msgbox (format ("You don't seem to have any Battle Points.\pPlease come back once you obtain some."))
	}
	else {
		msgbox(gText_HowMayIServeYou)
		waitmessage
		dynmultipush("Stones", 0)
		dynmultipush("Other", 1)
		dynmultipush("Extra", 2)
		dynmultistack(0, 0, FALSE, 3, FALSE, 0, DYN_MULTICHOICE_CB_NONE)
		if (var(VAR_RESULT) == 0) {
			special (ShowBattlePointsWindow)
			call (EvoStonesPokemart5)
		}
		elif (var(VAR_RESULT) == 1) {
			special (ShowBattlePointsWindow)
			call (EvoOtherPokemart5)
		}
		elif (var(VAR_RESULT) == 2) {
			special (ShowBattlePointsWindow)
			call (EvoExtraPokemart7)
		}
		special (CloseBattlePointsWindow)
		msgbox(gText_PleaseComeAgain)
	}
	closemessage
	release
	end
}

